!removes capslock, adds shift to capslock key, adds control to shift. wouldn't
!work with only keysym
keycode 66 = Shift_L
clear Lock
add Shift = Shift_L
keycode 50 = Control_L
remove Shift = Control_L
add Control = Control_L
!Awesome WM wants widnwoskey as modifier, alt, is easier to reach, swap
remove Mod4 = Super_L
remove Mod1 = Alt_L
!That wierd menu key on the right, opens a weirf version of th contextenu
keycode 135 = Super_L
add Mod4 = Alt_L
add Mod1 = Super_L
remove Mod1 = Alt_R
add Mod4 = Alt_R
keysym semicolon = colon semicolon
!control_L is now escape
keycode 37 = Escape
remove Control = Escape

